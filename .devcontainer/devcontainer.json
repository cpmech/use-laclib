{
  "name": "Laclib Open MPI",
  "image": "laclib_open_mpi",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash",
  },
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "forwardPorts": [
    9000
  ],
  "remoteUser": "vscode",
  "extensions": [
    "ms-vscode.cpptools"
  ]
}